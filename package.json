{
  "name": "todo_app",
  "version": "1.0.0",
  "description": "Laboratory work for MPP",
  "main": "index.js",
  "scripts": {
    "build": "cd ./nest-app && nest build",
    "format": "cd ./nest-app && prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cd ./nest-app && nest start",
    "start:dev": "cd ./nest-app && nest start --watch",
    "start:debug": "cd ./nest-app && nest start --debug --watch",
    "start:prod": "cd ./nest-app && node dist/main",
    "lint": "cd ./nest-app && eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KmXK/ToDo_Application.git"
  },
  "author": "KmXK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KmXK/ToDo_Application/issues"
  },
  "homepage": "https://github.com/KmXK/ToDo_Application#readme",
  "dependencies": {
    "@nestjs/typeorm": "^9.0.1",
    "hbs": "4.2.0",
    "mssql": "^7.3.5",
    "typeorm": "^0.3.12"
  }
}
